{
    "extends": "airbnb/index",
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "comma-dangle": [
            2,
            "never"
        ]
    },
    "env": {
        "es6": true,
        "browser": true,
        "mocha": true,
        "node": true
    }
}